package ex01;

public class main01 {

	public static void main(String[] args) {
		// 기존 자바 코드 => 객체(인스턴스) 사용하는 방법
		
		Greeter gt = new Greeter();
		
		gt.setFormat("%s님 안녕하세요");
		String msg = gt.greet("홍길동");
		
		System.out.println(msg);

		// 스프링에서의 객체 사용하는 방법
		// 스프링 컨테이너가 객체를 알아서 만들어서
		
	}

}
