package spring.calc;

public class ImpeCalculator implements Calculator {

	@Override
	public long facotorial(long num) { // 재귀 없이 팩토리얼 작성하는 법
		
		int result = 1;
		for(long i = 1; i<=num; i++) {
			result *= i;
		}
		
		System.out.println("ImpeCalculator.factorial(%d)의 결과 : " + result);
		return 0;
	}
	
}
